grade = 91

print("Your grade is")
 
if grade >= 90:
  print("A")
elif grade >= 80:
  print("B")
elif grade >= 70:
  print("C")
elif grade >= 60:
  print("D")
else:
  print("F")
  
  
  
  grade = 80

print("Your grade is")
 
if grade >= A:
  print("A")
elif grade >= B:
  print("B")
elif grade >= C:
  print("C")
elif grade >= D:
  print("D")
else:
  print("F")
